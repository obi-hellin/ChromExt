{
    "name" : "PRED202208",
    "description" : "Test version",
    "version" : "0.0.1",
    "manifest_version" : 3,
    "action" : {
        "default_icon" : "icon.png",
        "default_popup" : "popup.html"
    },
    "content_scripts" : [
        {
            "matches" : ["https://kream.co.kr/*"],
            "js" : ["index.js"],
            "css" : ["style2.css"]
        }
    ],
    "permissions" : [
        "tabs",
        "<all_urls>"
    ],
    "content_security_policy" : {
        "extension_pages" : "script-src 'self'; script-src-elem 'self' https://d3js.org/d3.v5.min.js; object-src 'self'"
    }
}