{
    "name": "Vue extension",
    "description": "My extension app made by Vue.js",
    "version": "0.0.1",
    "manifest_version": 3,
    "action": {
      "default_icon": "icon.png",
      "default_popup": "index.html"
    },
    "content_scripts": [
      {
        "matches" : ["https://kream.co.kr/products/*"],
        "js" : ["./index.js"]
      }
    ]
  }